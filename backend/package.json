{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "dev:control": "pnpm --filter control-api start:dev",
    "dev:app": "pnpm --filter app-api start:dev",
    "dev": "concurrently \"pnpm dev:control\" \"pnpm dev:app\"",
    "seed:plans": "ts-node scripts/seed-plans.ts",
    "prisma:generate:control": "prisma generate --schema prisma/control/schema.prisma",
    "prisma:generate:tenant": "prisma generate --schema prisma/tenant/schema.prisma",
    "prisma:generate:all": "pnpm prisma:generate:control && pnpm prisma:generate:tenant",
    "db:seed:control": "prisma db seed --schema prisma/control/schema.prisma",
    "db:seed:tenant": "ts-node prisma/tenant/seed.ts",
    "db:deploy:control": "prisma migrate deploy --schema prisma/control/schema.prisma",
    "db:deploy:tenant": "prisma migrate deploy --schema prisma/tenant/schema.prisma",
    "db:deploy:all": "pnpm db:deploy:control && pnpm db:deploy:tenant",
    "db:deploy:tenants": "ts-node scripts/deploy-tenants.ts",
    "fix:migration:tenant": "ts-node scripts/fix-tenant-migration.ts",
    "db:seed:all": "pnpm db:seed:control && pnpm db:seed:tenant",
    "db:studio": "prisma studio --schema prisma/control/schema.prisma",
    "backfill:tenants": "ts-node scripts/backfill-all-tenants.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/control/seed.ts"
  },
  "pnpm": {
    "overrides": {
      "typescript": "^5.4.0"
    },
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@prisma/client",
      "@prisma/engines",
      "@scarf/scarf",
      "bcrypt",
      "es5-ext",
      "fsevents",
      "msgpackr-extract",
      "prisma"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@prisma/client": "^6.12.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^24.1.0",
    "@types/pg": "^8.15.4",
    "prisma": "^6.12.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "bcrypt": "^6.0.0"
  }
}
